{
  "name": "remark-ui",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run dev",
    "build": "bash ./config/bin/build",
    "dev": "bash ./config/bin/dev",
    "lint": "bash ./config/bin/lint",
    "lint-js": "bash ./config/bin/lint-js",
    "prebuild": "npm run lint"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-transform-react-constant-elements": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@igoradamenko/eslint-config": "^0.1.0",
    "autoprefixer": "9.4.3",
    "babel-loader": "8.0.4",
    "babel-plugin-jsx-pragmatic": "1.0.2",
    "babel-plugin-transform-react-remove-prop-types": "0.4.19",
    "clean-webpack-plugin": "0.1.19",
    "css-loader": "2.1.0",
    "eslint": "5.3.0",
    "fibers": "3.1.1",
    "html-webpack-plugin": "2.30.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.0",
    "postcss-loader": "3.0.0",
    "postcss-url": "8.0.0",
    "postcss-wrap": "0.0.4",
    "preact-compat": "3.18.4",
    "prop-types": "15.6.2",
    "react-hot-loader": "4.3.11",
    "resolve-url-loader": "3.0.0",
    "sass": "1.15.2",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "webpack": "3.12.0",
    "webpack-dev-server": "2.7.1"
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0",
    "bem-react-helper": "^1.1.2",
    "redux-zero": "4.12.0",
    "preact": "8.3.1"
  },
  "engines": {
    "node": ">=8"
  },
  "lint-staged": {
    "linters": {
      "{src,config}/**/*.{js,jsx}": [
        "eslint --fix --cache -c .eslintrc.js",
        "git add"
      ]
    },
    "renderer": "silent"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  }
}
